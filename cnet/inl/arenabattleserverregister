	public:
		int server_type;
		int battle_type;
		int server_id;
		int world_tag;
		enum { PROTOCOL_TYPE = PROTOCOL_ARENABATTLESERVERREGISTER };
	public:
		ArenaBattleServerRegister() { type = PROTOCOL_ARENABATTLESERVERREGISTER; }
		ArenaBattleServerRegister(void*) : Protocol(PROTOCOL_ARENABATTLESERVERREGISTER) { }
		ArenaBattleServerRegister (int l_server_type,int l_battle_type,int l_server_id,
			int l_world_tag)
			 : server_type(l_server_type),battle_type(l_battle_type),server_id(l_server_id)
			,world_tag(l_world_tag)
		{
			type = PROTOCOL_ARENABATTLESERVERREGISTER;
		}

		ArenaBattleServerRegister(const ArenaBattleServerRegister &rhs)
			: Protocol(rhs),server_type(rhs.server_type),battle_type(rhs.battle_type),server_id(rhs.server_id)
			,world_tag(rhs.world_tag) { }

		GNET::Protocol *Clone() const { return new ArenaBattleServerRegister(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << server_type;
			os << battle_type;
			os << server_id;
			os << world_tag;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> server_type;
			os >> battle_type;
			os >> server_id;
			os >> world_tag;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
