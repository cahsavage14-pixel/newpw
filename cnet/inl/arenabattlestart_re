	public:
		int battle_id;
		short retcode;
		enum { PROTOCOL_TYPE = PROTOCOL_ARENABATTLESTART_RE };
	public:
		ArenaBattleStart_Re() { type = PROTOCOL_ARENABATTLESTART_RE; }
		ArenaBattleStart_Re(void*) : Protocol(PROTOCOL_ARENABATTLESTART_RE) { }
		ArenaBattleStart_Re (int l_battle_id,short l_retcode)
			 : battle_id(l_battle_id),retcode(l_retcode)
		{
			type = PROTOCOL_ARENABATTLESTART_RE;
		}

		ArenaBattleStart_Re(const ArenaBattleStart_Re &rhs)
			: Protocol(rhs),battle_id(rhs.battle_id),retcode(rhs.retcode) { }

		GNET::Protocol *Clone() const { return new ArenaBattleStart_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << battle_id;
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> battle_id;
			os >> retcode;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
